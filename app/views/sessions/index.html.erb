    <div class="jumbotron" id="intro-title">

      <div class="container">
        <h1>Agent Manual</h1>
        <p class="subtext">The only tool you'll need to become a successful real estate agent besides a personality.</p>
      </div>

      <div class="login"> 	
    		<% flash.each do |key,value|  %>
    			<div class="<%= key %>" > <%= value %> </div>
    		<% end %>
        <%= form_for(:agent, url: new_session_path, html: {class: "navbar-form"}) do |f| %>
          <div class="form-group login-field"><%= f.email_field :email, class: "form-control", placeholder: "Email" %></div>
          <div class="form-group login-field"><%= f.password_field :password, class: "form-control", placeholder: "Password" %></div>
          <%= f.submit "Login", id: "login-button", class: "btn btn-default" %>
            <button type="button" id="signup-button" class="btn btn-default" data-toggle="modal" data-target="#signUpModal">
              Sign Up
            </button>
        <% end %>
      </div>
  </div>


<!-- Modal content-->
  <div id="signUpModal" class="modal fade" role="dialog">
    <div class="modal-dialog">
      <div class="modal-content"  >
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">Create a New Account</h4>
        </div>
        <div class="modal-body">

          <% flash.each do |key,value|  %>
            <div class="<%= key %>" > <%= value %> </div>
          <% end %>

          <%= form_for(:agent, url: create_user_path, html: {class: "navbar-form"}) do |f| %>
        		<div><%= f.text_field :first_name, class: "form-control", placeholder: "First Name"  %></div>
        		<div><%= f.text_field :last_name, class: "form-control", placeholder: "Last Name"  %></div>
        		<div><%= f.text_field :phone, class: "form-control", placeholder: "Phone Number"  %></div>
        		<div><%= f.email_field :email, class: "form-control", placeholder: "Email"  %></div>
        		<div><%= f.password_field :password, class: "form-control", placeholder: "Password"  %></div>
        		<div><%= f.password_field :password_confirmation, class: "form-control", placeholder: "Confirm Password"  %></div>
        		<%= f.submit "Sign Up", class: "btn btn-default" %>
          <% end %>

        </div>
      </div>
    </div>
  </div>
<!-- Modal end -->